{
  "name": "podman-desktop-extension-mcp",
  "displayName": "MCP Server Manager",
  "description": "Model Context Protocol (MCP) Server Manager for Podman Desktop",
  "version": "0.0.1",
  "icon": "resources/icon.png",
  "publisher": "sparesparrow",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": ">=1.10.0"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsc --noEmitOnError false && vite build",
    "build:extension": "tsc --noEmitOnError false",
    "build:frontend": "vite build",
    "watch": "vite",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "contributes": {
    "id": "podman-desktop-extension-mcp",
    "views": {
      "podman-desktop-view": [
        {
          "id": "mcp-server-manager",
          "name": "MCP Server Manager",
          "when": "true"
        }
      ]
    }
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.13",
    "@podman-desktop/api": "1.16.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/svelte": "^5.2.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.3",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.7.3",
    "vite": "^5.0.12",
    "vitest": "^1.3.1",
    "ws": "^8.18.0",
    "@tailwindcss/typography": "^0.5.10"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1"
  },
  "packageManager": "pnpm@8.15.1",
  "type": "module"
}
